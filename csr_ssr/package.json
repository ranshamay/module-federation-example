{
  "name": "@server-side-rendering/shell",
  "description": "React 18 architecture at scale with module federation",
  "version": "1.0.0",
  "main": "server/index.js",
  "author": "Adam Hammad <mail@adamham.dev>",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && npm run serve",
    "serve": "node dist/server/main.js",
    "build": "npm run build:server && npm run build:client",
    "build:prod": "npm run build:prod:server && npm run build:prod:client",
    "build:client": "rimraf dist/client && webpack --progress --config config/webpack.client.js",
    "build:server": "rimraf dist/server && webpack --progress --config config/webpack.server.js",
    "build:prod:client": "rimraf dist/client && webpack --progress --config config/webpack.prod.client.js",
    "build:prod:server": "rimraf dist/server && webpack --progress --config config/webpack.prod.server.js"
  },
  "dependencies": {
    "@fluentui/react": "^8.103.3",
    "@fluentui/react-icons-mdl2": "^1.3.27",
    "@microsoft/1ds-analytics-web-js": "^3.2.8",
    "@module-federation/node": "^0.9.4",
    "axios": "^1.2.1",
    "css-loader": "^6.7.3",
    "express": "^4.17.1",
    "i18next": "^22.0.8",
    "i18next-http-backend": "^2.0.2",
    "lodash": "^4.17.21",
    "node-fetch": "^3.2.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.0.0",
    "react-i18next": "^12.0.0",
    "regenerator-runtime": "^0.13.2",
    "style-loader": "^3.3.1"
  },
  "devDependencies": {
    "@babel/cli": "7.17.10",
    "@babel/core": "7.18.2",
    "@babel/node": "7.17.10",
    "@babel/plugin-proposal-class-properties": "7.17.12",
    "@babel/plugin-proposal-json-strings": "7.17.12",
    "@babel/plugin-proposal-object-rest-spread": "7.18.0",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-syntax-import-meta": "7.10.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-react": "7.17.12",
    "babel-loader": "8.2.5",
    "rimraf": "3.0.2",
    "webpack": "5.73.0",
    "webpack-cli": "4.9.2",
    "webpack-merge": "5.8.0"
  }
}
